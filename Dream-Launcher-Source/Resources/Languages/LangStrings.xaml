<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
     xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!-- Screen 3: Checks -->
    <system:String x:Key="screen3_checking">Checking requirements...</system:String>
    <system:String x:Key="screen3_closeButton">CLOSE</system:String>
    <system:String x:Key="screen3_checkError1">An error has occurred! It appears that the Dream Launcher is not placed in the game directory. Please, put the executable and all files for this Launcher in the "&lt;game&gt;/Game/Bin" directory.</system:String>
    <system:String x:Key="screen3_checkError2">An error has occurred! This does not appear to be a copy of The Sims 3 installed from Steam! Only Steam copies of The Sims 3 are supported!</system:String>
    <system:String x:Key="screen3_checkError3">An error has occurred! Dream Launcher does not support this version of The Sims 3. Only the "1.67.2.024037" (Steam) version is supported.</system:String>

    <!-- Screen 4: Translate -->
    <system:String x:Key="screen4_dialogTitle">Would you like to patch the translation from PT-PT to PT-BR? If you don't play using the Portuguese language, this shouldn't affect you at all.</system:String>
    <system:String x:Key="screen4_dialogY">YES</system:String>
    <system:String x:Key="screen4_dialogU">DON'T ASK AGAIN</system:String>
    <system:String x:Key="screen4_dialogN">NO</system:String>

    <!-- Screen 5: Intel Fix -->
    <system:String x:Key="screen5_dialogText">It looks like your computer has a "%CPU%" CPU. It is likely that the game will not launch on this CPU because of its "BIG.little" Intel architecture. Would you like to apply the fix patch for this issue now?</system:String>
    <system:String x:Key="screen5_dialogY">YES</system:String>
    <system:String x:Key="screen5_dialogU">DON'T ASK AGAIN</system:String>
    <system:String x:Key="screen5_dialogN">NO</system:String>
    <system:String x:Key="screen5_Loading">Checking resources...</system:String>

    <!-- Screen 6: Launcher -->
