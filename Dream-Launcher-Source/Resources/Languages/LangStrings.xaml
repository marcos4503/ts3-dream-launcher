    <system:String x:Key="launcher_dlc_ep9">EP9 - University Life</system:String>
    <system:String x:Key="launcher_dlc_ep10">EP10 - Island Paradise</system:String>
    <system:String x:Key="launcher_dlc_ep11">EP11 - Into The Future</system:String>
    <system:String x:Key="launcher_dlc_sp1">SP1 - High End Loft</system:String>
    <system:String x:Key="launcher_dlc_sp2">SP2 - Fast Lane</system:String>
    <system:String x:Key="launcher_dlc_sp3">SP3 - Outdoor Living</system:String>
    <system:String x:Key="launcher_dlc_sp4">SP4 - Town Life</system:String>
    <system:String x:Key="launcher_dlc_sp5">SP5 - Master Suite</system:String>
    <system:String x:Key="launcher_dlc_sp6">SP6 - Katy Perry's Sweet Treats</system:String>
    <system:String x:Key="launcher_dlc_sp7">SP7 - Diesel</system:String>
    <system:String x:Key="launcher_dlc_sp8">SP8 - 70's, 80's, and 90's</system:String>
    <system:String x:Key="launcher_dlc_sp9">SP9 - Movie</system:String>

</ResourceDictionary>