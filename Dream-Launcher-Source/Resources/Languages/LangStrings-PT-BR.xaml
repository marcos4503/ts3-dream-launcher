    <system:String x:Key="launcher_tools_install">INSTALAR</system:String>
    <system:String x:Key="launcher_tools_open">ABRIR</system:String>
    <system:String x:Key="launcher_tools_running">RODANDO</system:String>
    <system:String x:Key="launcher_tools_installError">Houve um problema ao instalar a ferramenta requisitada. Por favor, verifique sua conexão com a Internet.</system:String>
