        </Grid>
        <Grid x:Name="restartPopUp" Visibility="Collapsed">
            <Border BorderThickness="0,0,0,0" Background="#D800041B"/>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="720">
                <TextBlock TextWrapping="Wrap" Text="{DynamicResource launcher_patches_restartRequired}" VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="{DynamicResource TheSimsSansSc}" Foreground="White" FontSize="18" TextAlignment="Center"/>
                <BeautyButton:BeautyButton x:Name="patchRestartButton" Content="{DynamicResource launcher_patches_restartRequiredButton}" HoverBackground="#FF3CE41A" Height="28" FontFamily="{DynamicResource TheSimsSansSc}" FontSize="16" Background="#FF20BF00" BorderBrush="#FF083100" Foreground="White" Cursor="Hand" Width="200" Margin="0,32,0,0"/>
            </StackPanel>
        </Grid>
        <Grid x:Name="logsViewerPopUp" Visibility="Collapsed">
            <Grid x:Name="logsViewerBg" Background="Black" Opacity="0.75"/>
            <Grid x:Name="logsViewerWindow" Margin="0,0,0,0" HorizontalAlignment="Center" VerticalAlignment="Center" Width="800" Height="600" RenderTransformOrigin="0.5,0.5">
                <Grid.RenderTransform>
                    <TransformGroup>
                        <ScaleTransform/>
                        <SkewTransform/>
                        <RotateTransform/>
                        <TranslateTransform/>
                    </TransformGroup>
                </Grid.RenderTransform>
                <Rectangle Margin="0,0,0,0" StrokeThickness="0" RadiusX="8" RadiusY="8" Fill="White"/>
                <Border BorderThickness="0,0,0,0" Grid.Row="0" Grid.ColumnSpan="2"
                    CornerRadius="8,8,0,0" BorderBrush="Black" Background="#FF4B9C52" VerticalAlignment="Top" Height="32" Margin="0,0,0,0">
                    <Label x:Name="logsViewerTitle" Content="Place Holder" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="{DynamicResource TheSimsSansSc}" Foreground="White" FontSize="20" FontWeight="Bold"/>
                </Border>
                <Grid Margin="0,32,0,0">
                    <ScrollViewer Margin="0,0,0,32">
                        <StackPanel x:Name="logsViewerList" VerticalAlignment="Top" Margin="8,0,8,0">
                            <TextBlock x:Name="logsViewerEmpty" TextWrapping="Wrap" Text="{DynamicResource launcher_cache_logsViewerEmpty}" Margin="0,16,0,0" HorizontalAlignment="Center" VerticalAlignment="Top" FontStyle="Normal" Foreground="#FF9A9A9A"/>
                        </StackPanel>
                    </ScrollViewer>
                    <Rectangle Height="0.5" VerticalAlignment="Bottom" Margin="0,0,0,32" StrokeThickness="0" Fill="Gray"/>
                    <TextBlock TextWrapping="Wrap" Text="{DynamicResource launcher_cache_logsViewerMsg}" TextAlignment="Center" VerticalAlignment="Bottom" Height="32" Padding="0,2,0,0" Margin="64,0,64,0" FontSize="10" Opacity="0.5"/>
                </Grid>
                <Button x:Name="logsViewerClose" HorizontalAlignment="Right" VerticalAlignment="Top" Width="28" Height="28" Margin="0,2,4,0" Padding="0,0,0,0" Cursor="Hand" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}">
                    <Image VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Source="/Resources/close-popup.png"/>
                </Button>
            </Grid>
        </Grid>
        <Grid x:Name="toastPopUp" VerticalAlignment="Top" Margin="0,8,0,0" HorizontalAlignment="Center" Width="800" Panel.ZIndex="5" Visibility="Collapsed">
            <Rectangle x:Name="toastBg" StrokeThickness="2" Fill="#E5002E88" Margin="4,0,4,0" RadiusX="8" RadiusY="8" Stroke="#FF000746"/>
            <TextBlock x:Name="toastMessage" TextWrapping="Wrap" Text="TextBlock" FontWeight="Bold" FontSize="14" HorizontalAlignment="Left" Margin="16,8,0,8" Width="650" Foreground="White"/>
            <BeautyButton:BeautyButton x:Name="toastDismiss" Content="{DynamicResource launcher_toast_dismiss}" HoverBackground="#FF7AB2F3" FontFamily="{DynamicResource TheSimsSansSc}" Background="#FF558FD2" BorderBrush="#FF11355F" Foreground="White" Margin="0,0,16,0" HorizontalAlignment="Right" VerticalAlignment="Center" Height="24" Padding="8,0,8,0"/>
        </Grid>
        <Grid x:Name="toastsHistory" HorizontalAlignment="Left" Width="255" Margin="0,0,0,36" Visibility="Collapsed">
            <Rectangle Margin="4,4,4,4" Fill="#FF94C6FF" StrokeThickness="0" RadiusX="8" RadiusY="8"/>
            <Border BorderThickness="0,0,0,0" VerticalAlignment="Top" CornerRadius="8,8,0,0" Height="32" Margin="4,4,4,0" Background="#FF4B9C52">
                <Label Content="{DynamicResource launcher_toastsHistoryTitle}" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontFamily="{DynamicResource TheSimsSansSc}" Foreground="White" FontSize="20" FontWeight="Bold" Margin="0,0,0,0"/>
            </Border>
            <ScrollViewer x:Name="toastHistoryScroll" Margin="4,36,4,4">
                <StackPanel x:Name="toastsHistoryContainer" VerticalAlignment="Top">
                    <Grid x:Name="toastHistEmpty" Height="108" Margin="0,16,0,0">
                        <Image Height="80" Width="80" VerticalAlignment="Top" Source="/Resources/ghost.png" Stretch="UniformToFill" Opacity="0.5"/>
                        <Label Content="{DynamicResource launcher_toastsHistoryEmpty}" VerticalAlignment="Bottom" HorizontalContentAlignment="Center" Foreground="White" FontWeight="Normal" Opacity="0.75" FontStyle="Normal"/>
                    </Grid>
                </StackPanel>
            </ScrollViewer>
        </Grid>
        <Grid x:Name="vaultTaskPopUp" Visibility="Collapsed">
            <Border BorderThickness="0,0,0,0" Background="#D800041B"/>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Width="720">
                <Image gif:ImageBehavior.AnimatedSource="/Resources/load.gif" gif:ImageBehavior.AnimationSpeedRatio="1.5" Width="48" Height="48" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,0,16" />
                <TextBlock x:Name="vaultTaskMessage" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalAlignment="Center" FontFamily="{DynamicResource TheSimsSansSc}" Foreground="White" FontSize="18" TextAlignment="Center" Text="Place Holder"/>
            </StackPanel>
        </Grid>
        <Grid x:Name="interactionBlocker" Background="Black" Opacity="0.75" Visibility="Collapsed"/>

    </Grid>
</Window>
