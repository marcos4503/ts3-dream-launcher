                        <Image x:Name="dlc_sp1" Height="24" Width="24" ToolTipService.InitialShowDelay="1000" ToolTipService.ShowDuration="10000" ToolTipService.BetweenShowDelay="15000" Source="/Resources/dlc_sp1s.png" Margin="19,0,0,0" Cursor="Hand" />
                        <Image x:Name="dlc_sp2" Height="24" Width="24" ToolTipService.InitialShowDelay="1000" ToolTipService.ShowDuration="10000" ToolTipService.BetweenShowDelay="15000" Source="/Resources/dlc_sp2s.png" Margin="5,0,0,0" Cursor="Hand" />
                        <Image x:Name="dlc_sp3" Height="24" Width="24" ToolTipService.InitialShowDelay="1000" ToolTipService.ShowDuration="10000" ToolTipService.BetweenShowDelay="15000" Source="/Resources/dlc_sp3s.png" Margin="5,0,0,0" Cursor="Hand" />
                        <Image x:Name="dlc_sp4" Height="24" Width="24" ToolTipService.InitialShowDelay="1000" ToolTipService.ShowDuration="10000" ToolTipService.BetweenShowDelay="15000" Source="/Resources/dlc_sp4s.png" Margin="5,0,0,0" Cursor="Hand" />
                        <Image x:Name="dlc_sp5" Height="24" Width="24" ToolTipService.InitialShowDelay="1000" ToolTipService.ShowDuration="10000" ToolTipService.BetweenShowDelay="15000" Source="/Resources/dlc_sp5s.png" Margin="5,0,0,0" Cursor="Hand" />
                        <Image x:Name="dlc_sp6" Height="24" Width="24" ToolTipService.InitialShowDelay="1000" ToolTipService.ShowDuration="10000" ToolTipService.BetweenShowDelay="15000" Source="/Resources/dlc_sp6s.png" Margin="5,0,0,0" Cursor="Hand" />
