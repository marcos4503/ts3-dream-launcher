<Window x:Class="Sims3Launcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sims3Launcher"
        mc:Ignorable="d"
        Title="Sims3Launcher" SizeToContent="WidthAndHeight" WindowStyle="None" Icon="/Resources/icon-32x.png" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowState="Normal" ShowInTaskbar="False" Background="{x:Null}" AllowsTransparency="True" IsHitTestVisible="False">
    <Grid Width="640" Height="300" HorizontalAlignment="Left" VerticalAlignment="Top" IsHitTestVisible="False">
        <Rectangle x:Name="tempBackground" StrokeThickness="0" Fill="#FFBF00D4" Visibility="Collapsed" IsHitTestVisible="False"/>
        <Image Source="/Resources/plumbot-decoration-double.png" Stretch="Uniform" VerticalAlignment="Bottom" Margin="0,0,0,70" IsHitTestVisible="False"/>
        <Image Source="/Resources/shined-logo.png" Stretch="Uniform" Width="300" Height="300" VerticalAlignment="Top" Margin="0,-24,0,0" IsHitTestVisible="False"/>
        <Image Width="256" Source="/Resources/name.png" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="0,64,0,0" IsHitTestVisible="False"/>

    </Grid>
</Window>
